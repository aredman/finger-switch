#include <iostream>

using namespace std;

int main;
{
	edge A;
}

class edge(point 1,point 2);
{
	point 1;
	point 2;
	double length;
	double angle1;
	double angle2;
}

class point
{
		//Angle measures from magnetometer, notated by Magnetometer 'n-1', 'n-2', 'n+1', 'n+2'
	double MagNm1;
	double MagNm2;
	double MagNp1;
	double MagNp2;
		//Angle measures from accelerometer, notated by Accelerometer 'n-1', 'n-2', 'n+1', 'n+2'
	double AccNm1;
	double AccNm2;
	double AccNp1;
	double AccNp2;

}
